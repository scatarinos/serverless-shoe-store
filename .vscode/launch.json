{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Typescript Local File",
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "args": [
                "${relativeFile}"
            ],
            "env": {
                "AWS_REGION": "eu-west-1",
                "AWS_PROFILE": "sergio",
            }
        },
        {
            "type": "python",
            "request": "launch",
            "name": "Python Local File",
            "program": "${file}",
            "console": "integratedTerminal",            
            "env": {
                "AWS_REGION": "eu-west-1",
                "AWS_PROFILE": "sergio",
            }
        }
    ]
}